<Window x:Class="WpfAppDemo.Layouts.DockPanelLayoutOfWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppDemo.Layouts"
        mc:Ignorable="d"
        Title="DockPanelLayoutOfWin" Height="300" Width="300">
    <DockPanel LastChildFill="False">
        <Button DockPanel.Dock="Left" >Left</Button>
        <Button DockPanel.Dock="Top">Top</Button>
        <Button DockPanel.Dock="Right">Right</Button>
        <Button DockPanel.Dock="Bottom">Bttom</Button>
        <TextBox Text="Mukund"></TextBox>
    </DockPanel>
</Window>
