<Window x:Class="WpfAppDemo.Layouts.CanwaslayoutWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppDemo.Layouts"
        mc:Ignorable="d"
        Title="CanwaslayoutWin" Height="300" Width="300">
    <Canvas>
        <Button Canvas.Left="5" Canvas.Top="5" Content="Top,Left" ></Button>
        <Button Canvas.Right="5" Canvas.Top="5" Content="Top,Right" ></Button>
        <Button Canvas.Right="5" Canvas.Bottom="5" Content="Bottom,Right"></Button>
        <Button Canvas.Left="5" Canvas.Bottom="5" Content="Bottom,Left"></Button>
    </Canvas>
</Window>
